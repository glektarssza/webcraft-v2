{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "id": "root",
    "name": "Root Configuration",
    "description": "Root configuration for the project.",
    "enabled": true,
    "dictionaries": [
        "en-ca",
        "en-gb",
        "en-common-misspellings",
        "en-gb-common-misspellings",
        "filetypes",
        "fonts",
        "git",
        "shell",
        "bash",
        "javascript",
        "typescript",
        "node",
        "npm",
        "html",
        "html-symbol-entities",
        "css",
        "fullstack",
        "networking-terms",
        "web-services",
        "computing-acronyms",
        "softwareTerms",
        "software-term-suggestions",
        "software-tools",
        "coding-compound-terms",
        "public-licenses",
        "git",
        "gaming-terms",
        "game-development",
        "lorem-ipsum",
        "!cryptocurrencies"
    ],
    "import": ["./.cspell/configs/project-dictionary.json"],
    "enableGlobDot": true,
    "useGitignore": true,
    "gitignoreRoot": ["."],
    "caseSensitive": false,
    "ignorePaths": [".git", "pnpm-lock.yaml"],
    "loadDefaultConfiguration": true,
    "unknownWords": "report-all",
    "readonly": true
}
